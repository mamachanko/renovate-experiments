{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "rebaseWhen": "behind-base-branch",
  "enabledManagers": [
    "gomod"
  ],
  "extends": [
    "config:recommended"
  ],
  "postUpdateOptions": [
    "gomodTidy"
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "semanticCommits": "enabled"
}
